{
    "name": "marcelweidum/filament-passkeys",
    "description": "Use passkeys in your filamentphp app",
    "keywords": [
        "MarcelWeidum",
        "laravel",
        "filament-passkeys",
        "filament",
        "filamentphp"
    ],
    "homepage": "https://github.com/marcelweidum/filament-passkeys",
    "support": {
        "issues": "https://github.com/marcelweidum/filament-passkeys/issues",
        "source": "https://github.com/marcelweidum/filament-passkeys"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "MarcelWeidum",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-passkeys": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "MarcelWeidum\\Passkeys\\": "src/",
            "MarcelWeidum\\Passkeys\\Database\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcelWeidum\\Passkeys\\PasskeysServiceProvider"
            ],
            "aliases": {
                "Passkeys": "MarcelWeidum\\Passkeys\\Facades\\Passkeys"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
